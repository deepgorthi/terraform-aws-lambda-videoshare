{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Action": [
              "elastictranscoder:Read*",
              "elastictranscoder:List*",
              "elastictranscoder:*Job",
              "elastictranscoder:*Preset",
              "s3:ListAllMyBuckets",
              "s3:ListBucket",
              "iam:ListRoles",
              "sns:ListTopics"
          ],
          "Effect": "Allow",
          "Resource": "*"
      },
      {
          "Effect": "Allow",
          "Action": [
              "logs:*"
          ],
          "Resource": "arn:aws:logs:*:*:*"
      },
      {
          "Effect": "Allow",
          "Action": [
              "s3:GetObject",
              "s3:PutObject"
          ],
          "Resource": "arn:aws:s3:::*"
      }
  ]
}